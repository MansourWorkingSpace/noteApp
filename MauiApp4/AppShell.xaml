<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp4.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp4.Views"
    Title="MauiApp4"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Acceuil"
        ContentTemplate="{DataTemplate local:Acceuil}"
        Route="Acceuil" />

    <ShellContent
        Title="Note"
        ContentTemplate="{DataTemplate local:Note}"
        Route="Note" />

    <ShellContent
        Title="Profil"
        ContentTemplate="{DataTemplate local:Profil}"
        Route="Profil" />

</Shell>
